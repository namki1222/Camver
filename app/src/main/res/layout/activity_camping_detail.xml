                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="캠핑장 바닥"/>
                <TextView
                    android:layout_marginTop="5dp"
                    android:textColor="#6b6b6b"
                    android:layout_width="wrap_content"
