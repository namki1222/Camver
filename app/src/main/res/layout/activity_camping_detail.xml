                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true"
                        android:src="@drawable/star_on_off_select"/>
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="12dp"
                        android:layout_height="wrap_content"
